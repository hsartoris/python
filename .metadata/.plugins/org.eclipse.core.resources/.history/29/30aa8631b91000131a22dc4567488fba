import math

print "I'll repeat after you, unless you say stop."
while (1):
    user_string = raw_input()
    if (user_string == 'stop' or user_string == 'Stop'):
        break
    print user_string
print 'Alright, I stopped.'

print (4.0/3.0) * math.pi * (3,959 ** 3)
