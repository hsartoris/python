def percentInc(x,y):
    return (x - y) / x

ns2006 = 62130.
ns2008 = 66310.
ns2010 = 69160.
ns2012 = 72560.

rs2006 = 93950.
rs2008 = 97970.
rs2010 = 100660.
rs2012 = 102190.

pns2008 = percentInc(ns2006, ns2008)
pns2010 = percentInc(ns2008, ns2010)
pns2012 = percentInc(ns2010, ns2012)
