def percentInc(x,y):
    return (x - y) / x

ns2006 = 62130.
ns2008 = 66310.
ns2010 = 69160.
ns2012 = 72560.

rs2006 = 93950.
rs2008 = 97970.
rs2010 = 100660.
rs2012 = 102190.

pns2008 = percentInc(ns2006, ns2008)
pns2010 = percentInc(ns2008, ns2010)
pns2012 = percentInc(ns2010, ns2012)

prs2008 = percentInc(rs2006, rs2008)
prs2010 = percentInc(rs2008, rs2010)
prs2012 = percentInc(rs2010, rs2012)

r2006 = rs2006/ns2006
r2008 = rs2008/ns2008
r2010 = rs2010/ns2010
r2012 = rs2012/ns2012

